    <style>
        #size {
            font-size: 12px;
        }

        input[type="text"] {
            height: 45%;
        }


        /* width */
        ::-webkit-scrollbar {
            width: 8px;
        }

        #main-container {
            padding: 10px !important;
        }

        /* Track */
        ::-webkit-scrollbar-track {
            box-shadow: inset 0 0 5px grey;
            border-radius: 10px;
        }

        /* Handle */
        ::-webkit-scrollbar-thumb {
            background: grey;
            border-radius: 10px;
        }

        /* Handle on hover */
        ::-webkit-scrollbar-thumb:hover {
            background: lightgrey;
        }

